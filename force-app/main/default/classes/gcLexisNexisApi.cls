    private HttpRequest buildBasicRequest(String methodEndpoint) {
        HttpRequest request = new HttpRequest();

        request.setMethod('GET');
        request.setEndpoint(this.defaultEndpointAzure + methodEndpoint);
        //request.setHeader('Authorization', 'Basic ' + EncodingUtil.base64Encode(Blob.valueOf(USERNAME + ':' + PASSWORD)));
        request.setHeader('Environment', 'Testing');
        request.setHeader('Ocp-Apim-Subscription-Key', AZURE_SUBSCRIPTION_KEY);
        request.setHeader('Content-Type', 'application/json');

        request.setTimeout(120000);

        return request;
    }

    /**
     * Method to parse and get response for Business Instant ID
     * @return @type VerificationResult
     */
    private VerificationResult sendHandleResponseBusiness(HttpRequest request) {
        HttpResponse response = new Http().send(request);
        System.debug('Response Code='+response.getStatusCode()); //Amit
        if (response.getStatusCode() != 200 && response.getStatusCode() != 201) {
            System.debug('###response.getBody() = ' + response.getBody());
            throw new gcIDVerificationException(response.getStatusCode(), response.getBody());
        }
        
        System.debug(JSON.serializePretty(response.getBody()));

        BusinessInstantIdResponse responseParsed = (BusinessInstantIdResponse)JSON.deserialize(response.getBody(), BusinessInstantIdResponse.class);

        VerificationResult result = new VerificationResult();
        if (responseParsed.BIID20ResponseEx.response.Result.CompanyResults.BusinessVerification != null) {
            BusinessVerification bvf = responseParsed.BIID20ResponseEx.response.Result.CompanyResults.BusinessVerification;
            result.description = bvf.Description;
            result.verificationIndex = bvf.Index;
        }
        if (responseParsed.BIID20ResponseEx.response.Result.CompanyResults.BusinessIds != null) {
            result.lexisNexisId = responseParsed.BIID20ResponseEx.response.Result.CompanyResults.BusinessIds.SeleID;
        }
        return result;
    }

    /**
     * Method to parse and get response for Individual Instant ID
     * @return @type VerificationResult
     */
    private VerificationResult sendHandleResponseIndividual(HttpRequest request) {
        HttpResponse response = new Http().send(request);
        System.debug('Response Code='+response.getStatusCode()); //Amit        
        if (response.getStatusCode() != 200 && response.getStatusCode() != 201) {
            System.debug('###response.getBody() = ' + response.getBody());
            throw new gcIDVerificationException(response.getStatusCode(), 'LexisNexisIndividual: ' + response.getBody());
        }
        System.debug(JSON.serializePretty(response.getBody()));

        IndividualInstantIdResponse responseParsed = (IndividualInstantIdResponse)JSON.deserialize(response.getBody(), IndividualInstantIdResponse.class);

        // returns null if no sufficient params supplied
        VerificationResult result = new VerificationResult();
        if(responseParsed.InstantIDResponseEx.response.Result.ComprehensiveVerification != null &&
                responseParsed.InstantIDResponseEx.response.Result.ComprehensiveVerification.ComprehensiveVerificationIndex != null){
            result.verificationIndex = String.valueOf(responseParsed.InstantIDResponseEx.response.Result.ComprehensiveVerification.ComprehensiveVerificationIndex);
            result.lexisNexisId = responseParsed.InstantIDResponseEx.response.Result.UniqueId;
        }
        return result;
    }

