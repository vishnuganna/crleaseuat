        public VerificationResult sendBusiness(Account account, List<Contact> contacts) {
            BusinessInstantID20Request businessInstantID20Request = new BusinessInstantID20Request();
            businessInstantID20Request.User = new User();

            businessInstantID20Request.Options = new Options().addAllWatchListsRequested();

            Company company = new Company(account);
            businessInstantID20Request.addCompany(company);

            List<AuthorizedRep> authorizedReps = new List<AuthorizedRep>();
            for (Contact c : contacts) {
                AuthorizedRep authorizedRep = new AuthorizedRep(c);
                authorizedReps.add(authorizedRep);
            }
