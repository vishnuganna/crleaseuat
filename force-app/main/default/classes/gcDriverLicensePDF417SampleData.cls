    public class wrapPDF417{
        public String barcode{get;set;}
        public map<String,String> elements{get;set;}

        public wrapPDF417(map<String,String> pElements)
        {
            elements = pElements;
            barcode = createBarcode(elements);
        }
    }

    public static string createBarcode(map<String,String> elements)
    {
        String c10 = String.fromCharArray(new list<Integer>{10}); //newline
        String c13 = String.fromCharArray(new list<Integer>{13}); //carriage return
        String c30 = String.fromCharArray(new list<Integer>{30}); //record separator
        String b = '@';
        b += c10;
        b += c30;
        b += c13;
        b += 'ANSI ';
        b += '123456';//IIN
        b += '09'; //AAMVA version
        b += '00'; //jurisdiction version
        b += '01'; //number of subfiles
        b += 'DL';
        b += '0031'; //offset

        String data = '';

        for(String key : elements.keySet())
        {
            String val = elements.get(key);
            if(val != null && !val.isWhitespace() && key.length() == 3)
            {
                data += key;
                data += val;
                data += c10;
            }
        }
        String strDataLength = String.valueOf(data.length());
        strDataLength = ('0').repeat(4-strDataLength.length()) + strDataLength;
        b += strDataLength; //length of data
        b += data;
        b += c13;
        b += c13;
        return b;
    }

