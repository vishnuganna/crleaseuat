            'DAC' => 'First Name',
            'DAD' => 'Middle Name',
            'DAG' => 'Mailing Street Address1',            
            'DAI' => 'Mailing City',
            'DAJ' => 'Mailing Jurisdiction Code', //2 characters
            'DAK' => 'Mailing Postal Code', //11 characters
            'DAQ' => 'License or ID Number',
            'DAU' => 'Height', //e.g. 72in, 180cm
            'DAY' => 'Eye Color', //3 characters

            'DBA' => 'License Expiration Date', //MMDDCCYY for US, CCYYMMDD for Canada            
            'DBB' => 'Date of Birth', //MMDDCCYY for US, CCYYMMDD for Canada
            'DBC' => 'Sex', //1 = male, 2 = female, 9 = unknown            
            'DBD' => 'License or ID Document Issue Date', //MMDDCCYY for US, CCYYMMDD for Canada

            'DCA' => 'Jurisdiction-Specific Vehicle Class',
            'DCB' => 'Jurisdiction-Specific Restrictions',
            'DCD' => 'Jurisdiction-Specific Endorsements',
            'DCF' => 'Document Discriminator',
            'DCG' => 'Country territory of issuance', // USA or CAN            
            'DCS' => 'Family Name',

            'DDE' => 'Family Name Truncation', //T = 'Truncated', N = 'Not Truncated', U = 'Unknown whether Truncated'
            'DDF' => 'First Names Truncation',
            'DDG' => 'Middle Names Truncation',
*/

