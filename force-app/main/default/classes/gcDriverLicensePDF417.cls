    public static Map<String, String> parse(String barcode)
    {
        Integer idxPos = barcode.indexOf('ANSI') + 19;
        Integer idxLen = idxPos + 4;
        System.debug(barcode);
        Integer subFileOffset = Integer.valueOf(barcode.substring(idxPos,idxPos+4));
        Integer subFileLength = Integer.valueOf(barcode.substring(idxPos+4,idxPos+8));

        String subFile = barcode.substring(subFileOffset,subFileOffset+subFileLength);

        map<String,String> parsedElements = new map<String,String>();

        list<String> listSeg = subFile.split('\n');

        for(String s : listSeg)
        {
            String left3 = s.left(3);
            if(mapElementDef.containsKey(left3))
            {
                String val = s.substring(3);
                val = val.trim();
                if(val.length() > 0)
                    parsedElements.put(left3,val);
            }
        }

        return parsedElements;
    }

    public static Contact createAccount(String barcode)
    {
        return createAccount(parse(barcode));
    }

    public static Contact createAccount(map<String,String> mapElements)
    {
        String street;
        String city;
        String state;
        String postalCode;

        String descr = '';

        map<String,String> mapKeyDef = mapElementDef;
        for(String key : mapElements.keySet())
        {
            String name = mapKeyDef.get(key);
            String value= mapElements.get(key);
            if(name == null || value == null)
                continue;

            descr = descr + key + ' / ' + name + ':'+value+'; ';
        }

        if(mapElements.containsKey('DAG'))
        {
            street = mapElements.get('DAG');
            if(mapElements.containsKey('DAH'))
                street = street + ' ' + mapElements.get('DAH');
            city = mapElements.get('DAI');
            state = mapElements.get('DAJ');
            postalCode = mapElements.get('DAK');
        }
        else if(mapElements.containsKey('DAL'))
        {
            street = mapElements.get('DAL');
            if(mapElements.containsKey('DAM'))
                street = street + ' ' + mapElements.get('DAM');
            city = mapElements.get('DAN');
            state = mapElements.get('DAO');
            postalCode = mapElements.get('DAP');
        }

        if(postalCode != null)
            postalCode = postalCode.left(9);
            
        String firstName;
        String lastName;
        String licenseNumber;
        String country;

        if(mapElements.containsKey('DAC'))
            firstName = mapElements.get('DAC');
        else if(mapElements.containsKey('DBP'))
            firstName = mapElements.get('DBP');
        else if(mapElements.containsKey('DCT'))
            firstName = mapElements.get('DCT');
        else
                firstName = 'NoFirstName';

        if(mapElements.containsKey('DAB'))
            lastName = mapElements.get('DAB');
        else if(mapElements.containsKey('DBO'))
            lastName = mapElements.get('DBO');
        else if(mapElements.containsKey('DCS'))
            lastName = mapElements.get('DCS');
        else
                lastName = 'NoLastName';
        if (mapElements.containsKey('DAQ')) {
            licenseNumber = mapElements.get('DAQ');
        }

        if (mapElements.containsKey('DCG')) {
            country = mapElements.get('DCG');
        }
        Date licenseExpirationDate;
        if (mapElements.containsKey('DBA')) {
            String licenseExpDateStr = mapElements.get('DBA');
            licenseExpirationDate = parseMMDDCCYY(licenseExpDateStr);
        }

        Date dateOfBirth;
        if (mapElements.containsKey('DBB')) {
            String dateOfBirthStr = mapElements.get('DBB');
            dateOfBirth = parseMMDDCCYY(dateOfBirthStr);
        }

        Contact acc = new Contact(
                    MailingStreet = street,
                    MailingCity = city,
                    MailingCountryCode = 'US',
                    MailingCountry = 'United States',
                    MailingStateCode = state,
                    MailingState= gcAspireApi.changeStateToAbbrev(state),
                    MailingPostalCode = postalCode,
                    FirstName = firstName,
                    LastName = lastName,
                    Driver_s_License__c = licenseNumber,
                    DL_Expire_Date__c = licenseExpirationDate,
                    Birthdate = dateOfBirth
        );
        return acc;
    }

    public static Date parseMMDDCCYY(String toParse){
        String month = toParse.substring(0, 2);
        String dateStr = toParse.substring(2, 4);
        String year = toParse.substring(4);
        String dateISO = month + '/' + dateStr + '/' + year;
        return Date.parse(dateISO);
    }

